{
  "name": "radon-ctt-ide-plugin",
  "version": "0.0.1",
  "displayName": "radon-ctt-ide-plugin",
  "description": "",
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "openapi-generator"
  ],
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:radon-ctt-ide-plugin.helloWorld"
  ],
  "main": "./out/extension.js",
